// Prisma schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  name       String?
  description String?
  avatar     String?
  color      String   @default("#888888")
  createdAt DateTime @default(now())
}
